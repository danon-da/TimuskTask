#include <iostream>

int num[50][2];
int  N, K;

int init()
{
	int i;
	num[1][0] = 1
		num[1][1] = 1;
	//Определяем сколько всего есть последовательностей, 
	//удовлетворяющих условию, что нет рядом стоящих единиц
	for (i = 2; i <= N; i++)
	{
		num[i][0] = num[i - 1][0] + num[i - 1][1];
		num[i][1] = num[i - 1][0];
	}
	//число всех допустимых последовательность образует последовательность фибоначчи
	//В ячейке num[N][0] хранится количество подходящих последовательностей, начинающихся c нуля
	//В ячейке num[N][1] хранится количество подходящих последовательностей, начинающихся c единицы
	//выводится всё количество
	return num[N][0] + num[N][1];
}

void print(int k, int n)
{
	if (n == 0)
		return;
	//Проверяется каждая цифра нужной последовательности
	//и последовательно выводится 0 или 1
	if (k < num[n][0])
	{
		std::cout << "0";
		print(k, n - 1);
	}
	else
	{
		std::cout << "1";
		print(k - num[n][0], n - 1);
	}
}

int main()
{
	//вводим длину последовательности и индекс
	std::cin >> N >> K;
	//узнаем, сколько последовательностей удовлетворяет условиям
	int n = init();
	//Проверяем на корректность
	if (K > n) {
		std::cout << "-1";
		return 0;
	}
	//выводим нужную последовательность
	print(K - 1, N);
	return 0;
}
