#include <iostream>

//строка максимальной длины N
char a[20001];
//строка максимальной длины 2N
char b[20003];

//функция для преобразования символов 16-ричного числа
//в числовое формат(т.е. если это число то оно таким и остается 
//а если буква то преобразует ее в число(например А = 10)
unsigned char hexToChar(char c)
{
    if (c >= '0' && c <= '9')
        return  c - '0';
    else
        return 10 + c - 'A';
}

//преобразует 2 символа шестнадцатиричного числа в числовое значение
unsigned char getHex(char* a)
{
    return 16 * (hexToChar(a[0])) + hexToChar(a[1]);
}

int main()
{
    int N;
    //вводим первую строку как массив символов и запоминаем размер строки
    scanf("%s%n", a, &N);
    //вводим 2 строку, так же как массив
    scanf("%s", b);
    //начальный пробел
    unsigned char p = 32;
    //XOR отношение между 32 и второй строкой
    unsigned char k = p ^ getHex(b);
    printf("%02X", (unsigned char)k);
    //проходим по всем парам чисел
    for (int i = 0; i < N; i += 2)
    {
        //ХOR операция между текущей парой символов из 1 строки и значением К,
        //также являющимся XOR между элементами 1 и 2 строки
        unsigned char d = k ^ getHex(a + i);
        //смотрим следующую пару
        k = d ^ getHex(b + i + 2);
        //выводим К(в шестнадцатиричном коде)
        printf("%02X", (unsigned char)k);
    }
}
