#include <stdio.h>
#include <iostream>

//массив для информации на полоске
char A[1000001];
int S[1000001];

//функция, возвращающая номер игрока, кто победит
//параметры: номер игрока кто ходит первым и границы полоски
int dfs(int s, int a, int b) {

    //вычисляем длину полоски
    int n = b - a, d = S[b] - S[a];
    //если строка состоит только из букв A или только из букв B
    if (d == n || d == 0)
        return d == n ? 1 : 2;
    //если полоска нечетной длины
    if (n / 2 % 2) 
        return 0;

    //переворачиваем с 1 стороны
    auto x1 = dfs(1 + s % 2, a, (a + b) / 2); 
    //переворачиваем с другой стороны
    auto x2 = dfs(1 + s % 2, (a + b) / 2, b); 
    //если функция остановилась на каком либо игроке то выводим его номер(он будет победившим)
    if (x1 == s || x2 == s)
        return s;
    //Если функция остановилась на том что полоска нечетная, выводим 0
    else if (x1 == 0 || x2 == 0)
        return 0;
    else
        //иначе вывводим номер игрока, который должен ходить следующим
        return 1 + s % 2;
}

int main(){
    int n, i = 0;
    //вводим длину полоски
    scanf("%d", &n);
    //заполняем информацию об обеих сторонах в один массив
    for (int s = 0; s < 2; s++)
        scanf("%s", A + s * n);
    for (int i = 1; i <= 2 * n; i++)
        //считаем сколько было А
        S[i] = S[i - 1] + (A[i - 1] == 'A');

    //запускаем функцию(первая ходит Алиса и далее начальная и конечная координаты полоски)
    int a = dfs(1, 0, n * 2);
    printf(a == 0 ? "Draw" : a == 1 ? "Alice" : "Bob");
}
